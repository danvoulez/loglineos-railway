{
  "name": "loglineos-consciousness-system",
  "version": "1.0.0",
  "description": "LogLineOS - Quantum-to-Cosmos Consciousness Operating System",
  "private": true,
  "packageManager": "npm@9.0.0",
  "main": "server.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run build:all",
    "build:all": "npm run build:motor && npm run build:api && npm run build:ui",
    "build:motor": "cd 1-Motor && cargo build --release",
    "build:api": "cd API && pip install -r requirements.txt",
    "build:ui": "cd 6-UI && npm install && npm run build",
    "test": "npm run test:motor && npm run test:api && npm run test:system",
    "test:motor": "cd 1-Motor && cargo test",
    "test:api": "cd API && python -m pytest",
    "test:system": "node scripts/test-consciousness.js",
    "bootstrap": "node scripts/booty-bootstrap.js",
    "deploy": "railway deploy",
    "logs": "railway logs",
    "db:migrate": "node scripts/db-migrate.js",
    "db:seed": "node scripts/db-seed.js",
    "lint": "eslint .",
    "typecheck": "tsc -b --verbose || tsc --noEmit",
    "format": "prettier --check .",
    "format:write": "prettier --write ."
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "ws": "^8.14.2",
    "pg": "^8.11.3",
    "uuid": "^9.0.1",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1",
    "axios": "^1.6.0",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "typescript": "^5.5.0",
    "eslint": "^9.10.0",
    "typescript-eslint": "^8.0.1",
    "prettier": "^3.3.3",
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "consciousness",
    "logline",
    "identity",
    "quantum",
    "cosmos",
    "timeline",
    "motor",
    "federation"
  ],
  "author": "Dan Voulez (@danvoulez)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/danvoulez/loglineos-railway.git"
  },
  "homepage": "https://loglineos.railway.app",
  "bugs": {
    "url": "https://github.com/danvoulez/loglineos-railway/issues"
  }
}